{
  "parser": "babel-eslint",
  "extends": ["react-app"],
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": ["prettier"],
  "rules": {
    "max-len": ["error", {
      "code": 100
    }],
    "prefer-promise-reject-errors": ["off"],
    "react/jsx-filename-extension": ["off"],
    "react/prop-types": ["warn"],
    "no-return-assign": ["off"],
    "prettier/prettier": "error",
    "eol-last": 1
  }
}
